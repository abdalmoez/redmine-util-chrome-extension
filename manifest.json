{
    "manifest_version": 2,
  
    "name": "Redmine utils",
    "description": "Usefull coloring redmine tool",
    "version": "1.0",
  
    "icons": { 
        "512": "icon.png"
    },
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "main.html"
    },
    
    "permissions": [
     "activeTab"
    ],
    "content_scripts": [
	    {
            "matches":["<all_urls>"],
            "js": [
                "page.inc.js"
                ]
	    }
	]
  }